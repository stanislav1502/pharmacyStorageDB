<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="330.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.magistraturasgi.pharmacystorage.dbControllers.InboundController">
    <children>
        <Label layoutX="14.0" layoutY="14.0" text="Inbound Shipment Details" />
        <Label layoutX="14.0" layoutY="49.0" text="Supplier ID:" />
        <TextField fx:id="supplierIdField" layoutX="120.0" layoutY="45.0" />
        <Label layoutX="14.0" layoutY="89.0" text="Product ID:" />
        <TextField fx:id="productIdField" layoutX="120.0" layoutY="85.0" />
        <Label layoutX="14.0" layoutY="129.0" text="Arrival Date &#10;(D-M-Y H:M:S):" />
        <TextField fx:id="arriveDateField" layoutX="120.0" layoutY="125.0" />
        <Label layoutX="14.0" layoutY="169.0" text="Status:" />
        <TextField fx:id="statusField" layoutX="120.0" layoutY="165.0" />
        <Label layoutX="14.0" layoutY="209.0" text="Quantity:" />
        <TextField fx:id="quantityField" layoutX="120.0" layoutY="205.0" />
        <Label layoutX="14.0" layoutY="249.0" text="Price:" />
        <TextField fx:id="priceField" layoutX="120.0" layoutY="245.0" />

        <Button layoutX="120.0" layoutY="289.0" mnemonicParsing="false" onAction="#insertInbound" text="Insert Inbound Shipment" />
    </children>
</AnchorPane>
