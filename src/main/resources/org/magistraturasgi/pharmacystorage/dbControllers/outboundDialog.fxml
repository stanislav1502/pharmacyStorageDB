<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="250.0" prefWidth="400.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.magistraturasgi.pharmacystorage.dbControllers.OutboundController">
    <children>
        <Label layoutX="14.0" layoutY="14.0" text="Outbound Shipment Details" />
        <Label layoutX="14.0" layoutY="49.0" text="Outbound ID:" />
