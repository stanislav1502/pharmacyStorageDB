        <TextField fx:id="shipperIdField" layoutX="120.0" layoutY="125.0" />
        <Label layoutX="14.0" layoutY="169.0" text="Product ID:" />
        <TextField fx:id="productIdField" layoutX="120.0" layoutY="165.0" />
        <Label layoutX="14.0" layoutY="209.0" text="Requested Date:" />
        <TextField fx:id="requestedDateField" layoutX="120.0" layoutY="205.0" />
        <Label layoutX="14.0" layoutY="249.0" text="Sent Date:" />
        <TextField fx:id="sentDateField" layoutX="120.0" layoutY="245.0" />
