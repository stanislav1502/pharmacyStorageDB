<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.magistraturasgi.pharmacystorage.MenuController">
   <children>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="100.0" spacing="10.0">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <children>
            <Label text="View DB &#10;tables" textAlignment="CENTER" />
            <Button mnemonicParsing="false" onAction="#onProductsButtonClick" prefHeight="28.0" prefWidth="72.0" text="Products" />
            <Button mnemonicParsing="false" onAction="#onClientsButtonClick" prefHeight="28.0" prefWidth="72.0" text="Clients" />
            <Button mnemonicParsing="false" onAction="#onSuppliersButtonClick" prefHeight="28.0" prefWidth="72.0" text="Suppliers" />
            <Button mnemonicParsing="false" onAction="#onShippersButtonClick" prefHeight="28.0" prefWidth="72.0" text="Shippers" />
            <Button mnemonicParsing="false" onAction="#onInboundsButtonClick" prefHeight="28.0" prefWidth="72.0" text="Inbound" />
            <Button mnemonicParsing="false" onAction="#onOutboundsButtonClick" prefHeight="28.0" prefWidth="72.0" text="Outbound" />
         </children>
      </VBox>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="300.0" spacing="10.0">
        <padding>
          <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </padding>
         <Label fx:id="tableNameText" text="Label" />
         <TableView fx:id="resultsTable" prefHeight="200.0" prefWidth="200.0">
           <columns>
           </columns>
         </TableView>
      </VBox>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="100.0" spacing="10.0">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <children>
            <Button fx:id="addBtn" disable="true" mnemonicParsing="false" onAction="#onAddBtnClick" text="Add record" />
             <Button fx:id="delBtn" disable="true" mnemonicParsing="false" onAction="#onDelBtnClick" text="Add record" />
         </children>
      </VBox>
      <VBox alignment="CENTER" prefWidth="100.0" spacing="10.0">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</HBox>
