<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<HBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.magistraturasgi.pharmacystorage.MenuController">
   <children>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="100.0" spacing="10.0">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" minHeight="40.0" minWidth="100.0" text="View DB &#10;tables" textAlignment="CENTER" textOverrun="CENTER_ELLIPSIS">
               <font>
                  <Font size="14.0" />
               </font></Label>
            <Button minHeight="40.0" minWidth="100.0" mnemonicParsing="false" onAction="#onProductsButtonClick" text="Products" />
            <Button minHeight="40.0" minWidth="100.0" mnemonicParsing="false" onAction="#onClientsButtonClick" text="Clients" />
            <Button minHeight="40.0" minWidth="100.0" mnemonicParsing="false" onAction="#onSuppliersButtonClick" text="Suppliers" />
            <Button minHeight="40.0" minWidth="100.0" mnemonicParsing="false" onAction="#onShippersButtonClick"  text="Shippers" />
            <Button minHeight="40.0" minWidth="100.0" mnemonicParsing="false" onAction="#onInboundsButtonClick"  text="Inbound" />
            <Button minHeight="40.0" minWidth="100.0" mnemonicParsing="false" onAction="#onOutboundsButtonClick" text="Outbound" />
         </children>
      </VBox>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="300.0" spacing="10.0">
        <padding>
          <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </padding>
         <Label fx:id="tableNameText" text="Label" />
         <TableView fx:id="resultsTable" prefHeight="200.0" prefWidth="200.0">
           <columns>
           </columns>
         </TableView>
      </VBox>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="100.0" spacing="10.0">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <children>
            <Button fx:id="addBtn" disable="true" minHeight="40.0" minWidth="100.0" mnemonicParsing="false" onAction="#onAddBtnClick" text="Add record" />
             <Button fx:id="delBtn" disable="true" minHeight="40.0" minWidth="100.0" mnemonicParsing="false" onAction="#onDelBtnClick" text="Add record" />
         </children>
      </VBox>
      <VBox alignment="CENTER" prefWidth="100.0" spacing="10.0">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</HBox>
